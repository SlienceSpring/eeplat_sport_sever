<script type="text/javascript">
	$(".btn-sideright").click(function(){
			$(this).toggleClass("active");
			$(this).parent().parent().parent().parent().siblings(".sideRight").animate({width:'toggle',right:'0'});
			
	$(".sideRight .nav-tabs > li.active").addClass("arrow_box");
	$(".sideRight .nav-tabs > li").click(function(){
		if($('.sideRight .nav-tabs > li').hasClass("active")){
			$(this).addClass("arrow_box");
			$(this).siblings().removeClass("arrow_box")
			}
		})
		$(".nav-sidebar > li > a").click(function(){
			if($(this).siblings("panel-collapse").hasClass("in")){
				$(this).parent().addClass("active")
				}
			
			
			})
		
		
		
	});
 </script>